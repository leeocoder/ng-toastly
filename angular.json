{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "ng-toastly": {
      "projectType": "library",
      "root": "projects/ng-toastly",
      "sourceRoot": "projects/ng-toastly/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "ng-packagr:build",
          "options": {
            "project": "projects/ng-toastly/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-toastly/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-toastly/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "ng-packagr:test",
          "options": {
            "tsConfig": "projects/ng-toastly/tsconfig.spec.json"
          }
        }
      }
    }
  }
}
